{
    "env": {
        "development": {
            "presets": [
                [
                    "babel-preset-env",
                    {
                        "targets": {
                            "node": "6.10",
                            "forceAllTransforms": true
                        }
                    }
                ],
                "react"
            ],
            "plugins": [
                [ "remove-react-element", { "elementNames": ["Desktop"] } ]
            ]
        },
        "module": {
            "presets": [
                [
                    "babel-preset-env",
                    {
                        "targets": {
                            "node": "6.10",
                            "forceAllTransforms": true
                        },
                        "modules": false
                    }
                ],
                "react"
            ]
        }
    },
    "sourceMaps": true
}